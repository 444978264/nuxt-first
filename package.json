{
  "name": "nuxt-study",
  "version": "1.0.0",
  "description": "nuxt study",
  "author": "yifang.chen",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "npm i && nuxt build",
    "bd": "nuxt build",
    "start": "node server/index.js",
    "generate": "nuxt generate",
    "lint": "cross-env NODE_ENV=development eslint --ext .js,.vue --fix --ignore-path .eslintignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "prod": "pm2 start pm2.config.js --env production",
    "analyze": "nuxt build -a",
    "reload": "pm2 reload demo --update-env",
    "component": "node config/template.js dir=components",
    "page": "node config/template.js dir=pages"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/component-cache": "^1.1.4",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/proxy": "^1.3.3",
    "body-parser": "^1.18.3",
    "consola": "^2.5.8",
    "cookieparser": "^0.1.0",
    "cross-env": "^5.2.0",
    "dotenv": "^7.0.0",
    "element-ui": "^2.6.3",
    "express": "^4.16.4",
    "js-cookie": "^2.2.0",
    "nuxt": "^2.4.0",
    "url-parse": "^1.4.7",
    "videojs-contrib-hls": "^5.15.0",
    "vue-video-player": "^5.0.2",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "0.0.1",
    "@nuxtjs/style-resources": "^0.1.2",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-cli": "^6.26.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.11.0",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.2",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.7.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "pm2": "^3.4.1",
    "prettier": "1.16.3",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.3",
    "vue-jest": "^3.0.4"
  }
}
